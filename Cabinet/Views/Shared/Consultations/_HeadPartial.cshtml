@using Cabinet.Models.CabinetViewModel.Consultations

@model ConsultationViewModel
<div class="row">
    <div>
        <input required="" type="hidden" asp-for="Examination.Head.Id">
    </div>
    <div class="col-xs-3 col-sm-3 col-lg-3 col-md-3">
        <div class="e-float-input">
            <input required="" type="text" asp-for="Examination.Head.Oculaire">
            <span class="e-float-line"></span>
            <label class="e-float-text" asp-for="Examination.Head.Oculaire">Oculaire: </label>
        </div>
    </div>  
</div>
<div class="row">
    <div class="col-xs-12 col-sm-12 col-lg-12 col-md-12">
        <div class="e-float-input">
            <input required="" type="text" asp-for="Examination.Head.Orl">
            <span class="e-float-line"></span>
            <label class="e-float-text" asp-for="Examination.Head.Orl">Orl: </label>
        </div>
    </div>
</div>