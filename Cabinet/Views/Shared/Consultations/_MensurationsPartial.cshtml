@using Cabinet.Models.CabinetViewModel.Consultations


@model ConsultationViewModel

<div class="row">
    <div>
        <input required="" type="hidden" asp-for="Mensuration.Id">
    </div>
    <div class="col-xs-3 col-sm-3 col-lg-3 col-md-3">
        <div class="e-float-input">
            <input required="" type="number" asp-for="Mensuration.Poids">
            <span class="e-float-line"></span>
            <label class="e-float-text" asp-for="Mensuration.Poids">Poids: </label>
        </div>
    </div>

    <div class="col-xs-3 col-sm-3 col-lg-3 col-md-3">
        <div class="e-float-input">
            <input required="" type="number" asp-for="Mensuration.PercentilPoids">
            <span class="e-float-line"></span>
            <label class="e-float-text" asp-for="Mensuration.PercentilPoids">Percentil: </label>
        </div>
    </div>

    <div class="col-xs-3 col-sm-3 col-lg-3 col-md-3">
        <div class="e-float-input">
            <input required="" type="number" asp-for="Mensuration.Taille">
            <span class="e-float-line"></span>
            <label class="e-float-text" asp-for="Mensuration.Taille">Taille: </label>
        </div>
    </div>
    <div class="col-xs-3 col-sm-3 col-lg-3 col-md-3">
        <div class="e-float-input">
            <input required="" type="number" asp-for="Mensuration.PercentilTaille">
            <span class="e-float-line"></span>
            <label class="e-float-text" asp-for="Mensuration.PercentilTaille">Percentil: </label>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-xs-3 col-sm-3 col-lg-3 col-md-3">
        <div class="e-float-input">
            <input required="" type="number" asp-for="Mensuration.Pc">
            <span class="e-float-line"></span>
            <label class="e-float-text" asp-for="Mensuration.Pc">Pc: </label>
        </div>
    </div>

    <div class="col-xs-3 col-sm-3 col-lg-3 col-md-3">
        <div class="e-float-input">
            <input required="" type="number" asp-for="Mensuration.PercentilPc">
            <span class="e-float-line"></span>
            <label class="e-float-text" asp-for="Mensuration.PercentilPc">Percentil en DS: </label>
        </div>
    </div>

    <div class="col-xs-3 col-sm-3 col-lg-3 col-md-3">
        <div class="e-float-input">
            <input required="" type="number" asp-for="Mensuration.Temperature">
            <span class="e-float-line"></span>
            <label class="e-float-text" asp-for="Mensuration.Temperature">Temperature: </label>
        </div>
    </div>
    <div class="col-xs-3 col-sm-3 col-lg-3 col-md-3">
        <div class="e-float-input">
            <input required="" type="number" asp-for="Mensuration.Imc">
            <span class="e-float-line"></span>
            <label class="e-float-text" asp-for="Mensuration.Imc">IMC: </label>
        </div>
    </div>
</div>